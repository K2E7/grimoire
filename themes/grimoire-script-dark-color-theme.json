{
    "name": "Grimoire: Script Dark",
    "type": "dark",
    "colors": {
        "foreground": "#E4E0D8BF",
        "focusBorder": "#2C7385C7",
        "activityBar.inactiveForeground": "#545454",
        "activityBar.foreground": "#C9C5BEBF",
        "activityBarBadge.background": "#C9C5BE",
        "activityBarBadge.foreground": "#1C1C1C",
        "editor.background": "#161616",
        "editor.lineHighlightBorder": "#323232",
        "editor.selectionBackground": "#54545433",
        "editor.inactiveSelectionBackground": "#54545422",
        "editorStickyScroll.background": "#181818",
        "editorStickyScroll.shadow": "#121212",
        "editorBracketHighlight.foreground1": "#588495C7",
        "editorBracketHighlight.foreground2": "#945761C7",
        "editorBracketHighlight.foreground3": "#949157C7",
        "editorBracketHighlight.foreground4": "#588495C7",
        "editorBracketHighlight.foreground5": "#945761C7",
        "editorBracketHighlight.foreground6": "#949157C7",
        "editorBracketMatch.border": "#323232",
        "editorBracketHighlight.unexpectedBracket.foreground": "#E67881",
        "editorGroupHeader.tabsBackground": "#1C1C1C",
        "editorGroupHeader.noTabsBackground": "#1C1C1C",
        "editorGutter.addedBackground": "#566F56",
        "editorGutter.deletedBackground": "#AB5960",
        "editorGutter.modifiedBackground": "#C19C00",
        "editorIndentGuide.background1": "#323232B5",
        "editorIndentGuide.activeBackground1": "#404040",
        "editorLineNumber.foreground": "#C9C5BE27",
        "editorLineNumber.activeForeground": "#C9C5BEBF",
        "editorRuler.foreground": "#C9C5BE27",
        "sideBar.foreground": "#E4E0D8BF",
        "sideBarSectionHeader.foreground": "#C9C5BE",
        "statusBar.background": "#181818",
        "statusBar.foreground": "#C9C5BEBF",
        "statusBarItem.remoteBackground": "#C9C5BE",
        "statusBarItem.remoteForeground": "#1C1C1C",
        "tab.activeBackground": "#161616",
        "tab.activeForeground": "#E4E0D8E6",
        "tab.inactiveForeground": "#C9C5BEBF",
        "tab.activeBorder": "#181818",
        "tab.activeBorderTop": "#E4E0D8BF",
        "tab.inactiveBackground": "#1C1C1C",
        "titleBar.activeForeground": "#C9C5BEBF",
        "titleBar.inactiveForeground": "#C9C5BEBF"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "comment.block",
                "comment.line",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#404040",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.operator.new",
                "keyword.operator.expression",
                "storage.type"
            ],
            "settings": {
                "foreground": "#9988C8E6",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#F8BE98E0"
            }
        },
        {
            "scope": [
                "string.template"
            ],
            "settings": {
                "foreground": "#25B0BCE7"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#E05374E6"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "support.function"
            ],
            "settings": {
                "foreground": "#25B0BCE7"
            }
        },
        {
            "scope": [
                "variable",
                "variable.other.constant",
                "variable.other.readwrite",
                "variable.parameter",
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#E05374E6"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#F8BE98E0"
            }
        },
        {
            "scope": [
                "variable.other.enummember",
                "variable.other.property",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#C16FA0"
            }
        },
        {
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#E05374E6"
            }
        },
        {
            "scope": [
                "meta.tag"
            ],
            "settings": {
                "foreground": "#E4E0D8E6"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#C19C00E7",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "keyword.operator",
                "punctuation"
            ],
            "settings": {
                "foreground": "#E4E0D8BF"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "keyword": "#9988C8E6",
        "function": "#25B0BCE7",
        "method": "#25B0BCE7",
        "variable": "#E05374E6",
        "parameter": "#E05374E6",
        "property": "#C16FA0",
        "class": "#F8BE98E0",
        "interface": "#F8BE98E0",
        "type": "#F8BE98E0",
        "enum": "#F8BE98E0",
        "enumMember": "#C16FA0",
        "string": "#F8BE98E0",
        "number": "#E05374E6",
        "operator": "#E4E0D8BF"
    }
}